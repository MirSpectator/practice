<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.money.dao.UserMapper">
    <select id="listObjects" resultType="com.money.entity.User">
        select  * from user
    </select>

    <select id="getUserByPhone" resultType="com.money.entity.User">
        select  * from user where  phone = #{phone}
    </select>
    
</mapper>